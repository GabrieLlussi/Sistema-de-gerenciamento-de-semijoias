{% extends 'base.html.twig' %}
{% block title %}{{ titulo }}{% endblock %}
{% block body %}
<h1>{{titulo}}</h1>
{{ form_start(form) }}
    <div class="container text-left">
            <div class="row align-items-start">
                <div class="col-md-4">
                    {{ form_row(form.nome) }}
                </div>
            </div>
        </div>
{{ form_end(form) }}

<p> {{ msg }} </p>
<a href ="{{ path('categoria') }}" class="btn btn-primary">Voltar ao menu</a>
{% endblock %}